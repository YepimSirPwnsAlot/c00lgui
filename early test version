New = function(Object, Parent, Name, Data)
	
	local Object = Instance.new(Object)
	if not Data then
		for Index,Value in pairs({}) do
			Object[Index] = Value
		end
		Object.Parent = Parent
		Object.Name = Name
		return Object
	end
end

