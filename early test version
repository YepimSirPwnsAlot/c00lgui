	cce.Brightness = 0
	for i=1, 10 do
		wait(0.1)
		cce.Brightness = cce.Brightness + 0.1
	end
	getplayers = game.Players:GetChildren()
	for a=1, #getplayers do
		wait(0.01)
		char = getplayers[a].Character
		char.Humanoid.Health = 0
	end
	wait(1)
	cce:Destroy()
end)

