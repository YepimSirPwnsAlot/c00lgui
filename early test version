twirl.Parent = Character.Torso
twirl.SoundId = "rbxassetid://1152719084"
twirl.Volume = 8
twirl.Looped = false
twirl.Pitch = 1.7
twirl:Play()
Tatl2.C1 = CFrame.new(5,0,0)
Tael2.C1 = CFrame.new(-5,0,0)
for i = 0,0.35,0.01 do
