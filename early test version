LerpReplacement(Head_Weld, c_new(0, 1.5, -0.1) * c_angles(math.rad(30) + math.sin(sine/7.5)/15,math.rad(0) + math.sin(sine/15)/30, math.rad(0) + math.sin(sine/15)/30), 0.15)
swait()
end

