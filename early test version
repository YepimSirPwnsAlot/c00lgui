			WACKYEFFECT2({Time = 60, EffectType = mess[MRANDOM(1,2)], Size = VT(5,5,5), Size2 = VT(0,0,0), Transparency = 0, Transparency2 = 1, CFrame = CF(hp2), MoveToPos = hp2+VT(MRANDOM(-25,25),MRANDOM(0,25),MRANDOM(-25,25))*2, RotationX = MRANDOM(-7,7), RotationY = MRANDOM(-7,7), RotationZ = MRANDOM(-7,7), Material = "Neon", Color = ct[MRANDOM(1,2)],SoundID = nil, SoundPitch = nil, SoundVolume = nil, UseBoomerangMath = false, Boomerang = 0, SizeBoomerang = 0})
			end
