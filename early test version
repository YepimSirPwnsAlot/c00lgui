LerpReplacement(RA_Weld, c_new(1.5, 0.5, -0.25) * c_angles(math.rad(20) - math.sin(sine/7.5)/15, math.rad(30) + math.sin(sine/7.5)/15,math.rad(80) + math.sin(sine/7.5)/15), 0.15)
