	spm.MeshType = "Sphere"
	spm.Scale = Vector3.new(100,100,100)
	sp2 = Instance.new("Part", rarm)
	sp2.Name = "Energy"
	sp2.BrickColor = BrickColor.new("Royal purple")
	sp2.Size = Vector3.new(1,1,1)
	sp2.Shape = "Ball"
