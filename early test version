local function CreateMOTD()
	local messages = {
		"MastersMZ friend me on discord: yepimnythic",
		"Added way to much things! Enjoy the new stuff!",
		"Thanks for using my old one MastersMZ!",
		"Idk",
		"No MOTD Found :(",
		"you may need to rejoin to stop the Su Tart music."
	}

	local selectedMessage = messages[math.random(1, #messages)]

	if not selectedMessage then
		Welcome2.Text = "No MOTD Was Found :("
	else
		Welcome2.Text = selectedMessage
	end
end

CreateMOTD()

