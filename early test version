  	rh.C0 = rh.C0:lerp(CF(0.5,-0.5,-0.4)*ANGLES(RAD(19.5),RAD(-0.1),RAD(1.3)),Alpha)
  	la.C0 = la.C0:lerp(CF(-1.5,0.1,-0.2)*ANGLES(RAD(71.2),RAD(0),RAD(-24.2)),Alpha)
  	ra.C0 = ra.C0:lerp(CF(1.5,0.1,-0.2)*ANGLES(RAD(71.2),RAD(0),RAD(18.7)),Alpha)
  	Neck.C0 = Neck.C0:lerp(NECKC0*CF(0,0,0+ ((1) - 1))*CF(0,0,0)*ANGLES(RAD(-9.7),RAD(0),RAD(0)),Alpha)
	end
	ATTACK = false
end

function dalightningbal()
	ATTACK = true
	for i = 0,50 do
		Swait()
		  	local Alpha = .1
	righthw.C0 = righthw.C0:lerp(rightcf*CF(0,0,7),.1)
  	RootJoint.C0 = RootJoint.C0:lerp(CF(0.1,3,-0.2)*ANGLES(RAD(0),RAD(63.6),RAD(0))*ROOTC0,Alpha)
  	lh.C0 = lh.C0:lerp(CF(-0.4,-1,0)*ANGLES(RAD(-4.5),RAD(0.3),RAD(-3.3)),Alpha)
  	rh.C0 = rh.C0:lerp(CF(0.5,-0.5,-0.6)*ANGLES(RAD(-5.3),RAD(-0.1),RAD(1.3)),Alpha)
  	la.C0 = la.C0:lerp(CF(-1.5,0.6,0.1)*ANGLES(RAD(0),RAD(0),RAD(-24.2)),Alpha)
  	ra.C0 = ra.C0:lerp(CF(1.4,0.4,-0.1)*ANGLES(RAD(83.5),RAD(-0.4),RAD(65.5)),Alpha)
  	Neck.C0 = Neck.C0:lerp(CF(0,0,0+ ((1) - 1))*CF(-0.4,0,0.1)*ANGLES(RAD(-21.1),RAD(-62),RAD(-18.8))*NECKC0,Alpha)
	end
	local invs = CreatePart(3,Effects,"Neon",0,1,"White","p",VT(5,5,5),true)
