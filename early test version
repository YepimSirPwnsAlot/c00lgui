			end
			end
		end))
	end
	for i = 0,15 do
		Swait()
		  	local Alpha = .2
	lefthandw.C0 = lefthandw.C0:lerp(leftcf*CF(0,2,5)*ANGLES(RAD(20),0,0),.2)
	righthw.C0 = righthw.C0:lerp(rightcf*CF(0,2,5)*ANGLES(RAD(20),0,0),.2)
  	RootJoint.C0 = RootJoint.C0:lerp(CF(0,0.1,-0.4)*ANGLES(RAD(-24.8),RAD(0),RAD(0))*ROOTC0,Alpha)
