CFrame.new(-0.649998546, -2.32730579, 0.445138097, 0.939692736, 0, 0.342019886, -0.116977625, 0.939692736, 0.321393639, -0.321393639, -0.342019886, 0.883022428),
}, .4, false)
end
RATr.Enabled = true
Humanoid.WalkSpeed = 0
