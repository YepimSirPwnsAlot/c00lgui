--dmg

function DEATH(MODEL)
	MODEL:BreakJoints()
	local ct = {C3(1,1,1),C3(0,0,0)}
	for index, CHILD in pairs(MODEL:GetChildren()) do
		if CHILD:IsA("BasePart") and CHILD.Name ~= "HumanoidRootPart" then
			if CHILD.Name == "Head" then
					WACKYEFFECT2({Time = 80, EffectType = "Box", Size = VT(CHILD.Size.Z,CHILD.Size.Y,CHILD.Size.Z), Size2 = (VT(CHILD.Size.Z,CHILD.Size.Y,CHILD.Size.Z)*3), Transparency = CHILD.Transparency, Transparency2 = 1, CFrame = CHILD.CFrame, MoveToPos = CHILD.Position+VT(MRANDOM(0,50),MRANDOM(0,50),MRANDOM(0,50)), RotationX = MRANDOM(-4,4), RotationY = MRANDOM(-4,4), RotationZ = MRANDOM(-4,4), Material = "Neon",ColorLoop = true, Color = ct[MRANDOM(1,2)], SoundID = nil, SoundPitch = nil, SoundVolume = nil, UseBoomerangMath = true, SizeBoomerang = 5, Boomerang = 35})
				elseif CHILD.Name ~= "HumanoidRootPart" then
					WACKYEFFECT2({Time = 80, EffectType = "Box", Size = CHILD.Size, Size2 = CHILD.Size*3, Transparency = CHILD.Transparency, Transparency2 = 1, CFrame = CHILD.CFrame, MoveToPos = CHILD.Position+VT(MRANDOM(0,50),MRANDOM(0,50),MRANDOM(0,50)), RotationX = MRANDOM(-4,4), RotationY = MRANDOM(-4,4), RotationZ = MRANDOM(-4,4) ,Material = "Neon", Color = ct[MRANDOM(1,2)], SoundID = nil, SoundPitch = nil, SoundVolume = nil, UseBoomerangMath = true, SizeBoomerang = 5, Boomerang = 35})
				end
			CHILD:remove()
		end
		end
	Debris:AddItem(MODEL,6)
end

function ApplyDamage(Humanoid,Damage)
	Damage = Damage
	if Humanoid.Health < 2000 then
		if Humanoid.Health - Damage > 0 then
			Humanoid.Health = Humanoid.Health - Damage
		else
			DEATH(Humanoid.Parent)
		end
	else
		DEATH(Humanoid.Parent)
	end
end

function ApplyAoE(POSITION,RANGE,MINDMG,MAXDMG,FLING,INSTAKILL)
	for index, CHILD in pairs(workspace:GetDescendants()) do
		if CHILD.ClassName == "Model" and CHILD ~= Character then
			local HUM = CHILD:FindFirstChildOfClass("Humanoid")
			if HUM then
				local TORSO = CHILD:FindFirstChild("Torso") or CHILD:FindFirstChild("UpperTorso")
				if TORSO then
					if (TORSO.Position - POSITION).Magnitude <= RANGE then
						if INSTAKILL == true then
							DEATH(CHILD)
						else
							local DMG = MRANDOM(MINDMG,MAXDMG)
							ApplyDamage(HUM,DMG)
						end
						if FLING > 0 then
							for _, c in pairs(CHILD:GetChildren()) do
								if c:IsA("BasePart") then
									local bv = Instance.new("BodyVelocity") 
									bv.maxForce = Vector3.new(1e9, 1e9, 1e9)
									bv.velocity = CF(POSITION,TORSO.Position).lookVector*FLING
									bv.Parent = c
									Debris:AddItem(bv,0.05)
								end
							end
						end
					end
				end
			end
		end
	end
end

--attacks
function threecolumns()
	ATTACK = true
	for i = 0,20 do
		Swait()
		  	local Alpha = .1
	lefthandw.C0 = lefthandw.C0:lerp(leftcf*CF(0,-10,7)*ANGLES(RAD(-140),0,0),.1)
	righthw.C0 = righthw.C0:lerp(rightcf*CF(0,-10,7)*ANGLES(RAD(-140),0,0),.1)
  	RootJoint.C0 = RootJoint.C0:lerp(CF(0,0,0.3)*ANGLES(RAD(25.4),RAD(0),RAD(0))*ROOTC0,Alpha)
  	lh.C0 = lh.C0:lerp(CF(-0.4,-1,0.2)*ANGLES(RAD(-30),RAD(0.3),RAD(-3.3)),Alpha)
  	rh.C0 = rh.C0:lerp(CF(0.5,-0.8,-0.6)*ANGLES(RAD(-30.7),RAD(-0.1),RAD(1.3)),Alpha)
  	la.C0 = la.C0:lerp(CF(-1.5,0.8,0.2)*ANGLES(RAD(-159.5),RAD(0),RAD(-24.2)),Alpha)
  	ra.C0 = ra.C0:lerp(CF(1.5,0.7,0.3)*ANGLES(RAD(-159.5),RAD(0),RAD(18.7)),Alpha)
  	Neck.C0 = Neck.C0:lerp(NECKC0*CF(0,0,0+ ((1) - 1))*CF(0,0,0.2)*ANGLES(RAD(-9.7),RAD(0),RAD(0)),Alpha)
	end
	local hf,hp = Raycast(RootPart.Position, (CF(RootPart.Position, RootPart.Position + VT(0, -1, 0))).lookVector, 3.7, Character)
	if hf then
		coroutine.resume(coroutine.create(function()
			for i = 1,8 do
				swait(.15)
			local po = RootPart.CFrame*CF(0,0,-i*22)
			local hf2,hp2 = Raycast(po.p+VT(0,1,0), (CF(RootPart.Position, RootPart.Position + VT(0, -1, 0))).lookVector, 3.7, Character)
			if hf then
			local ab = CF(hp2)*CF(0,100,0)
			SpawnTrail(ab.p,hp2,true)
			SpawnTrail(ab.p,hp2,false)
			local ct = {C3(1,1,1),C3(0,0,0)}
			local mess = {"Sphere","Box"}
			WACKYEFFECT2({Time = 60, EffectType = "Sphere", Size = VT(60,.1,60), Size2 = VT(75,.1,75), Transparency = 0, Transparency2 = 1, CFrame = CF(hp2), MoveToPos = nil, RotationX = 0, RotationY = 0, RotationZ = 0, Material = "Neon", Color = C3(0,0,0),SoundID = 231917806, SoundPitch = 1, SoundVolume = 5, UseBoomerangMath = false, Boomerang = 0, SizeBoomerang = 0})
			WACKYEFFECT2({Time = 60, EffectType = "Sphere", Size = VT(50,.1,50), Size2 = VT(65,.1,65), Transparency = 0, Transparency2 = 1, CFrame = CF(hp2), MoveToPos = nil, RotationX = 0, RotationY = 0, RotationZ = 0, Material = "Neon", Color = C3(1,1,1),SoundID = 192410084, SoundPitch = 1.3, SoundVolume = 5, UseBoomerangMath = false, Boomerang = 0, SizeBoomerang = 0})
			WACKYEFFECT2({Time = 30, EffectType = "Sphere", Size = VT(10,10,10), Size2 = VT(57,57,57), Transparency = 0, Transparency2 = 1, CFrame = CF(hp2), MoveToPos = nil, RotationX = 0, RotationY = 0, RotationZ = 0, Material = "Neon", Color = C3(0,0,0),SoundID = 243711369, SoundPitch = 1, SoundVolume = 5, UseBoomerangMath = false, Boomerang = 0, SizeBoomerang = 0})
			WACKYEFFECT2({Time = 30, EffectType = "Sphere", Size = VT(10,10,10), Size2 = VT(47,47,47), Transparency = 0, Transparency2 = 1, CFrame = CF(hp2), MoveToPos = nil, RotationX = 0, RotationY = 0, RotationZ = 0, Material = "Neon", Color = C3(1,1,1),SoundID = nil, SoundPitch = nil, SoundVolume = nil, UseBoomerangMath = false, Boomerang = 0, SizeBoomerang = 0})
			WACKYEFFECT2({Time = 40, EffectType = "Swirl", Size = VT(50,1,50)/3, Size2 = VT(50,100,50)/3, Transparency = 0, Transparency2 = 1, CFrame = ab, MoveToPos = ab.p-VT(0,30,0), RotationX = 0, RotationY = 7, RotationZ = 0, Material = "Neon", Color = C3(0,0,0),SoundID = nil, SoundPitch = nil, SoundVolume = nil, UseBoomerangMath = false, Boomerang = 0, SizeBoomerang = 0})
			for i = 1,3 do
			WACKYEFFECT2({Time = 50, EffectType = "Round Slash", Size = VT(.01,.01,.01), Size2 = VT(.5,.1,.5), Transparency = 0, Transparency2 = 1, CFrame = CF(hp2)*ANGLES(RAD(MRANDOM(-360,360)),RAD(MRANDOM(-360,360)),RAD(MRANDOM(-360,360))), MoveToPos = hp2+VT(0,6,0), RotationX = 0, RotationY = MRANDOM(-3,3), RotationZ = 0, Material = "Neon", Color = ct[MRANDOM(1,2)],SoundID = nil, SoundPitch = nil, SoundVolume = nil, UseBoomerangMath = false, Boomerang = 0, SizeBoomerang = 0})
			end
