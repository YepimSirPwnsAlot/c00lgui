local Object = game:GetObjects("rbxassetid://6099241563")[1]  
Object.Parent = game.Workspace  
script = Object.xesterv2

local Player = game.Players.LocalPlayer
local Mouse,mouse,UserInputService,ContextActionService
do
	local CAS = {Actions={}}
	local fakeEvent = function()
		local t = {_fakeEvent=true}
		t.Connect = function(self,f)self.Function=f end
		t.connect = t.Connect
		return t
	end
	local m = {Target=nil,Hit=CFrame.new(),KeyUp=fakeEvent(),KeyDown=fakeEvent(),Button1Up=fakeEvent(),Button1Down=fakeEvent()}
	local UIS = {InputBegan=fakeEvent(),InputEnded=fakeEvent()}
	function CAS:BindAction(name,fun,touch,...)
		CAS.Actions[name] = {Name=name,Function=fun,Keys={...}}
	end
	function CAS:UnbindAction(name)
		CAS.Actions[name] = nil
	end
	local function te(self,ev,...)
		local t = m[ev]
		if t and t._fakeEvent and t.Function then
			t.Function(...)
		end
	end
	m.TrigEvent = te
	UIS.TrigEvent = te
	local userMouse = Player:GetMouse()
	game:GetService("RunService").RenderStepped:Connect(function()
		m.Target = userMouse.Target
		m.Hit = userMouse.Hit
	end)
	game:GetService("UserInputService").InputBegan:Connect(function(io,gpe)
		if gpe then return end
		if io.UserInputType == Enum.UserInputType.MouseButton1 then
			m:TrigEvent("Button1Down")
		else
			for n,t in pairs(CAS.Actions) do
				for _,k in pairs(t.Keys) do
					if k==io.KeyCode then
						t.Function(t.Name,io.UserInputState,io)
					end
				end
			end
			m:TrigEvent("KeyDown",io.KeyCode.Name:lower())
			UIS:TrigEvent("InputBegan",io,false)
		end
	end)
	game:GetService("UserInputService").InputEnded:Connect(function(io,gpe)
		if gpe then return end
		if io.UserInputType == Enum.UserInputType.MouseButton1 then
			m:TrigEvent("Button1Up")
		else
			for n,t in pairs(CAS.Actions) do
				for _,k in pairs(t.Keys) do
					if k==io.KeyCode then
						t.Function(t.Name,io.UserInputState,io)
					end
				end
			end
			m:TrigEvent("KeyUp",io.KeyCode.Name:lower())
			UIS:TrigEvent("InputEnded",io,false)
		end
	end)
	Mouse,mouse,UserInputService,ContextActionService = m,m,UIS,CAS
end

