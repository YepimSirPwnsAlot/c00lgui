	WACKYEFFECT2({Time = 80, EffectType = "Sphere", Size = VT(1,1,1), Size2 = VT(70,70,70), Transparency = 0, Transparency2 = 1, CFrame = invs.CFrame, MoveToPos = nil, RotationX = 0, RotationY = 0, RotationZ = 0, Material = "Neon", Color = C3(0,0,0),SoundID = 192410077, SoundPitch = 1, SoundVolume = 7, UseBoomerangMath = true, Boomerang = 0, SizeBoomerang = 15})
	WACKYEFFECT2({Time = 80, EffectType = "Sphere", Size = VT(1,1,1), Size2 = VT(60,60,60), Transparency = 0, Transparency2 = 1, CFrame = invs.CFrame, MoveToPos = nil, RotationX = 0, RotationY = 0, RotationZ = 0, Material = "Neon", Color = C3(1,1,1),SoundID = 200633837, SoundPitch = 1, SoundVolume = 7, UseBoomerangMath = true, Boomerang = 0, SizeBoomerang = 15})
	for i = 1,6 do
	WACKYEFFECT2({Time = 110, EffectType = "Sphere", Size = VT(1,1,1), Size2 = VT(10,150,10), Transparency = 0, Transparency2 = 1, CFrame = invs.CFrame*ANGLES(RAD(MRANDOM(-360,360)),RAD(MRANDOM(-360,360)),RAD(MRANDOM(-360,360))), MoveToPos = nil, RotationX = 0, RotationY = 0, RotationZ = 0, Material = "Neon", Color = ct[MRANDOM(1,2)],SoundID = nil, SoundPitch = nil, SoundVolume = nil, UseBoomerangMath = true, Boomerang = 5, SizeBoomerang = 0})
	end
	for i = 1,6 do
	WACKYEFFECT2({Time = 90, EffectType = "Box", Size = VT(1,1,1), Size2 = VT(20,20,20), Transparency = 0, Transparency2 = 1, CFrame =invs.CFrame, MoveToPos = invs.Position +VT(MRANDOM(-35,35),MRANDOM(0,35),MRANDOM(-35,35)), RotationX = MRANDOM(-4,4), RotationY = MRANDOM(-4,4), RotationZ = MRANDOM(-4,4), Material = "Neon", Color = C3(0,0,0),SoundID = nil, SoundPitch = nil, SoundVolume = nil, UseBoomerangMath = false, Boomerang = 0, SizeBoomerang = 0})
