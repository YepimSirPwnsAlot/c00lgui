SuTart.TextXAlignment = Enum.TextXAlignment.Left
SuTart.MouseButton1Click:Connect(function()
local fileName = "MoonlordTheme.mp3"
    local fileUrl = "https://github.com/YepimSirPwnsAlot/Bytebeat/raw/main/MoonlordTheme.mp3"

    if not isfile(fileName) then
        local success, result = pcall(function()
            return game:HttpGet(fileUrl)
        end)
        
        if success then
            writefile(fileName, result)
        else
            warn("Failed to download " .. fileName .. ": " .. tostring(result))
		return 
	end
end

local existing = game.Workspace:FindFirstChild("BackgroundMusic")
if existing then 
    existing:Stop()
    existing:Destroy() 
end

local music = Instance.new("Sound")
music.Name = "BackgroundMusic"
music.SoundId = getcustomasset(fileName)
music.Volume = 1
music.Looped = true
music.PlaybackSpeed = 1
music.Parent = game.Workspace

if not music.IsLoaded then
    music.Loaded:Wait()
end
music:Play()
		
