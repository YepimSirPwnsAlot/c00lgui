bodyvel.Velocity = RootPart.CFrame.lookVector*50
local dasu = 0
local par = rayCast(RootPart.Position,RootPart.CFrame.lookVector*1,5,chara)
repeat
dasu = dasu + 1
bodyvel.Velocity = RootPart.CFrame.lookVector*50
par = rayCast(RootPart.Position,RootPart.CFrame.lookVector*1,5,chara)
