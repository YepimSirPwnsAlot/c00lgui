LerpReplacement(RA_Weld, c_new(0.8, 0.5, -0.65) * c_angles(math.rad(50) + math.sin(sine/15)/35,math.rad(-5) - math.sin(sine/15)/35,math.rad(-76) + math.sin(sine/15)/15), 0.15)
