	for i = 1, 60, 2 do
		rng2.CFrame = rng2.CFrame * CFrame.Angles(0,math.rad(5) + i*math.random(1,10),math.rad(0))
		rngm2.Scale = Vector3.new(5 + i*0.3, 2  - i*0.1 ,5 + i*0.3)
		rng2.Transparency = i/30
		rng2.CFrame = rng2.CFrame - Vector3.new(0,0.2,0)
		swait()
	end
	wait()
	rng2:Destroy()
end)
