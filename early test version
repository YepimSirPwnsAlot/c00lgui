	WACKYEFFECT2({Time = 30, EffectType = "Sphere", Size = VT(1,1,1), Size2 = VT(1,40,1), Transparency = 0, Transparency2 = 1, CFrame = righthand.circle.CFrame*ANGLES(RAD(90),RAD(90),RAD(i*20)), MoveToPos = nil, RotationX = 0, RotationY = 0, RotationZ = 0, Material = "Neon", Color = C3(1,1,1),SoundID = nil, SoundPitch = nil, SoundVolume = nil, UseBoomerangMath = false, Boomerang = 0, SizeBoomerang = 0})
