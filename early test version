function dashattack()
attack = true
Humanoid.WalkSpeed = 2
so(487911937,Head,1,3)
for i=0,2,.1 do
swait()
PlayAnimationFromTable({
CFrame.new(0, -0.260801345, 0.37769106, 1, 0, 0, 0, 0.939692736, -0.342019886, 0, 0.342019916, 0.939692736),
