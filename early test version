		for i = 0,1,0.005 do
			game:GetService("RunService").Heartbeat:Wait()
			fire1weld.C0 = fire1weld.C0:Lerp(CFrame.new(0,-10,0),0.017)
			fire2weld.C0 = fire2weld.C0:Lerp(CFrame.new(0,-10,0),0.017)
